{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_SUPABASE_URL": "@react_app_supabase_url",
    "REACT_APP_SUPABASE_ANON_KEY": "@react_app_supabase_anon_key",
    "REACT_APP_SUPABASE_SERVICE_ROLE": "@react_app_supabase_service_role",
    "REACT_APP_VOICEFLOW_PROJECT_ID": "@react_app_voiceflow_project_id",
    "REACT_APP_VOICEFLOW_VERSION_ID": "@react_app_voiceflow_version_id",
    "REACT_APP_VOICEFLOW_API_KEY": "@react_app_voiceflow_api_key"
  }
}
